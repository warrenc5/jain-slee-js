                            <cleanupDaemonThreads>false</cleanupDaemonThreads>
                            <executable>${executable}</executable>
                            <arguments>
                                <argument>-agentlib:native-image-agent=config-output-dir=${basedir}/target/native-image/</argument>
                                <argument>-cp</argument>
                                <classpath/>
                                <!-- 
                                <argument>
                                    -illegal-access=warn
                                </argument>
                                -->
                                <argument>
                                    run.RunScript
                                </argument>
                                
                                <argument>--debug=${js.debug}</argument>
                                <argument>--trace=${js.trace}</argument>
                                <argument>--username=${js.username}</argument>
                                <argument>--password=${js.password}</argument>
                                <argument>--url=${js.url}</argument>
                                <argument>${js.script}</argument>
                            </arguments>
                        </configuration>
                        <dependencies>
                            <dependency>
                                <groupId>${project.groupId}</groupId>
                                <artifactId>${project.artifactId}</artifactId>
                                <version>${project.version}</version>
                            </dependency>
                        </dependencies>
                    </plugin>
                </plugins>
            </build>
