        <profile>
            <id>native</id>
            <activation>
                <activeByDefault>false</activeByDefault>
            </activation>
            <properties>
                <skipTests>true</skipTests>
            </properties>
            <build>
                <defaultGoal>install</defaultGoal>
                <plugins>
                    <plugin>
                        <groupId>org.codehaus.mojo</groupId>
                        <artifactId>build-helper-maven-plugin</artifactId>
                        <executions>
                            <execution>
                                <id>add-source</id>
                                <phase>generate-sources</phase>
                                <goals>
                                    <goal>add-source</goal> 
                                </goals>
                                <configuration>
                                    <sources>
                                        <source>src/main/svm</source>
                                    </sources>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                    <plugin>
                        <groupId>org.graalvm.buildtools</groupId>
                        <artifactId>native-maven-plugin</artifactId>
                        <version>${native.maven.plugin.version}</version>
                        <extensions>true</extensions>
                        <executions>
                            <execution>
                                <id>build-native</id>
                                <goals>
                                    <goal>build</goal>
                                </goals>
                                <phase>package</phase>
                            </execution>
                        </executions>
                        <configuration>
                            <skip>false</skip>
                            <imageName>${imageName}</imageName>
                            <classpath>
                                <param>${project.build.directory}/jmxjs-${project.version}-shaded.jar</param>
                                <param>${WILDFLY_HOME}/modules/system/layers/base/org/mobicents/ss7/modules/main/*</param>
                                <param>${WILDFLY_HOME}/modules/system/layers/base/org/mobicents/ss7/commons/main/*</param>
                                <param>${WILDFLY_HOME}/modules/system/layers/base/org/restcomm/diameter/lib/main/</param>
                                <param>${WILDFLY_HOME}/modules/system/layers/base/org/restcomm/smpp/bootstrap/main/*</param>
                            </classpath>
                            <buildArgs>
                                --language:js
                                --enable-all-security-services
                                -H:ClassInitialization=:build_time
                                -H:+ReportExceptionStackTraces
                                -H:-TruffleCheckBlockListMethods
                                -Dtruffle.TruffleRuntime=com.oracle.truffle.api.impl.DefaultTruffleRuntime
                                --no-fallback
                                --diagnostics-mode
                                --report-unsupported-elements-at-runtime

                                -H:IncludeResources=logging.properties
                                -H:IncludeResources=log4j.properties
                                -H:IncludeResources=META-INF/services/java.lang.SecurityManager
                                -H:IncludeResources=META-INF/services/java.security.Provider
                                -H:IncludeResources=META-INF/services/javax.security.sasl.SaslClientFactory
                                -H:IncludeResources=META-INF/services/javax.security.sasl.SaslClientFactory

                                --initialize-at-build-time=org.wildfly.security.WildFlyElytronProvider
                                --initialize-at-build-time=org.wildfly.security.sasl.util.PrivilegedSaslClientFactory
                                --initialize-at-build-time=org.wildfly.security.sasl.util.PrivilegedSaslClient
                                --initialize-at-build-time=org.wildfly.security.sasl.digest.DigestClientFactory

                                --initialize-at-build-time=org.jboss.remotingjmx.RemotingConnectorProvider
                                --initialize-at-build-time=com.sun.jmx.mbeanserver.JmxMBeanServer

                                --initialize-at-run-time=org.xnio.Xnio
                                --initialize-at-run-time=org.xnio.DefaultXnioWorkerHolder
                                --initialize-at-run-time=org.xnio.Xnio$MBeanHolder
